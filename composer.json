{
	"name": "friendsoftypo3/openid",
	"type": "typo3-cms-extension",
	"keywords": ["typo3","openid"],
	"description": "OpenID authentication for TYPO3 CMS",
	"license": ["GPL-2.0-or-later"],
	"homepage": "https://github.com/FriendsOfTYPO3/openid",
	"authors": [
		{
			"name": "Dmitry Dulepov",
			"email": "dmitry.dulepov@gmail.com",
			"role": "Maintainer"
		}
	],
	"require": {
		"ext-gmp": "*",
		"ext-curl": "*",
		"ext-dom": "*",
		"typo3/cms-core": "^9.5 || ^10.0",
		"typo3/cms-setup": "*"
	},
	"replace": {
		"typo3/cms-openid": "self.version",
		"typo3-ter/openid": "self.version"
	},
	"autoload": {
		"psr-4": {
			"FoT3\\Openid\\": "Classes/"
		}
	}
}
